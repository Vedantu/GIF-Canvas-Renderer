{
  "name": "gif-canvas-renderer",
  "version": "1.0.0",
  "description": "library to parse gif and render gif flawlessly on canvas",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "coverage": "jest --config jestconfig.json --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "clean": "rimraf dist",
    "tsc": "./node_modules/typescript/bin/tsc",
    "babel": "NODE_ENV=production babel dist --out-dir dist --copy-files",
    "build": "npm run clean && npm run tsc && npm run babel",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vedantu/GIFParser.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Vedantu/GIFParser/issues"
  },
  "homepage": "https://github.com/Vedantu/GIFParser#readme",
  "dependencies": {
    "gifuct-js": "^2.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  }
}
